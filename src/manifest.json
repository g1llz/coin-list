{
  "name": "Coinlist",
  "description": "The easiest cryptocurrency whitelist",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/coin16.png",
      "32": "/images/coin32.png",
      "48": "/images/coin48.png",
      "128": "/images/coin128.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "persist": false
  }
}
